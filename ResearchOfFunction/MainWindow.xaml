<Window x:Class="ResearchOfFunction.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Исследование функции" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Grid.Row = "0">
            <Button Grid.Row = "0" VerticalAlignment = "Center" HorizontalAlignment = "Center" FontSize = "20" Height ="40" Width = "350" Click ="Button_Click">Функция одномерной оптимизации</Button>
        </Grid>
        <Grid Grid.Row = "1">
            <Button Grid.Row = "1" VerticalAlignment = "Center" HorizontalAlignment = "Center" FontSize = "20" Height ="40" Width = "350" Click ="Multi_Click">Функция многомерной оптимизации</Button>
        </Grid>
    </Grid>
</Window>
